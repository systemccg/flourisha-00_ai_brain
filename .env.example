# ============================================================
# FLOURISHA AI BRAIN - Unified Environment Configuration
# ============================================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control
# Last Updated: 2025-12-23

# ============================================================
# SUPABASE CONFIGURATION
# ============================================================
SUPABASE_URL=https://db.leadingai.info
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# ============================================================
# FIREBASE CONFIGURATION (Web UI)
# ============================================================
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-firebase-auth-domain.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-firebase-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-firebase-storage-bucket.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-firebase-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

# ============================================================
# WEB API CONFIGURATION
# ============================================================
VITE_API_URL=http://100.66.28.67:8001

# ============================================================
# YOUTUBE CREDENTIALS (OAuth)
# ============================================================
# Path to YouTube OAuth2 credentials file
# Download from Google Cloud Console: https://console.cloud.google.com/
YOUTUBE_OAUTH_CREDENTIALS_PATH=/root/flourisha/00_AI_Brain/credentials/youtube_oauth.json

# YouTube API credentials (if using direct token)
YOUTUBE_CLIENT_ID=your-youtube-client-id
YOUTUBE_CLIENT_SECRET=your-youtube-client-secret
YOUTUBE_REDIRECT_URI=http://localhost:8080/callback

# YouTube tokens directory (contains refresh tokens for specific channels)
YOUTUBE_TOKENS_DIR=/root/flourisha/00_AI_Brain/credentials/youtube_tokens

# ============================================================
# CLICKUP INTEGRATION
# ============================================================
# ClickUp API Token - Get from: https://app.clickup.com/settings/apps
CLICKUP_API_KEY=your-clickup-api-key

# ClickUp Team/Workspace ID
# Find in URL: https://app.clickup.com/[TEAM_ID]/...
CLICKUP_TEAM_ID=your-team-id

# ClickUp OAuth Credentials (for Make.com / custom apps)
CLICKUP_CLIENT_ID=your-clickup-client-id
CLICKUP_CLIENT_SECRET=your-clickup-client-secret
CLICKUP_REDIRECT_URL=https://leadingai.info

# Claude Code API Token (for autonomous agents)
CLAUDE_CODE_OAUTH_TOKEN=your-claude-code-token

# ============================================================
# AIRTABLE INTEGRATION
# ============================================================
# Airtable Personal Access Token
# Get from: https://airtable.com/account/tokens
AIRTABLE_API_KEY=your-airtable-api-key

# Airtable Base ID (project/workspace identifier)
AIRTABLE_BASE_ID=your-base-id

# ============================================================
# NEO4J GRAPH DATABASE
# ============================================================
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password
NEO4J_DATABASE=neo4j

# ============================================================
# GMAIL INTEGRATION
# ============================================================
# Gmail OAuth credentials (for email integration)
GMAIL_CLIENT_ID=your-gmail-client-id
GMAIL_CLIENT_SECRET=your-gmail-client-secret
GMAIL_REFRESH_TOKEN=your-gmail-refresh-token

# ============================================================
# DOCUMENT PROCESSING
# ============================================================
# Docling API endpoint for document extraction
DOCLING_API_URL=http://localhost:8000

# ============================================================
# SERVER CONFIGURATION
# ============================================================
SERVER_IP=66.94.121.10
SERVER_HOSTNAME=leadingai.info
SERVER_PUBLIC_URL=https://leadingai.info

# ============================================================
# DOCKER & SANDBOX CONFIGURATION
# ============================================================
SANDBOX_IMAGE=flourisha-sandbox:latest
SANDBOX_RAM=2g
SANDBOX_CPU=2
SANDBOX_STORAGE_PATH=/tmp/sandbox-storage
SANDBOX_NETWORK_PREFIX=flourisha-sandbox-net

# ============================================================
# LOGGING & DEBUG
# ============================================================
LOG_LEVEL=INFO
DEBUG=false

# ============================================================
# API KEYS (Third-party services)
# ============================================================
# Add any other API keys here as needed
# Example:
# SERVICE_API_KEY=your-api-key
